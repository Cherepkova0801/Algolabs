def numbers_square(numbers):
    new_numbers = sorted([n ** 2 for n in numbers])
    
    return new_numbers  
    
def test_numbers_square_func(self):
        self.assertEqual(numbers_square([-4, -2, 0, 1, 3]), [0, 1, 4, 9, 16])  
        self.assertEqual(numbers_square([1, 2, 3, 4, 5]), [1, 4, 9, 16, 25])  
